{
    "name": "myExtension2",
    "displayName": "My Extension 2",
    "version": "1.0.0",
    "icon": "icon.ico",
    "publisher": "MayCoDev",
    "engines": {
      "vscode": "^1.32.0"
    },
    "activationEvents": [
      "onCommand:myExtension2.helloWorld"
    ],
    "main": "extension.js",
    "contributes": {
      "commands": [
        {
          "command": "myExtension2.helloWorld",
          "title": "Hello World",
          "category": "HelloWorld"
        }
      ],
      "iconThemes": [
        {
          "id": "CGTheme",
          "label": "CG Theme",
          "path": "./fileIcons/cgIconTheme.json"
        }
      ],
      "languages": [
        {
          "id": "html",
          "icon": {
            "light": "./fileIcons/html_file.png",
            "dark": "./fileIcons/html_file.png"
          }
        },
        {
          "id": "js",
          "icon": {
            "light": "./fileIcons/js_file.png",
            "dark": "./fileIcons/js_file.png"
          }
        }
      ]
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/CGWebDev2003/vscode-extension-test"
    }
  }